{
  "name": "react-project",
  "version": "0.1.0",
  "dependencies": {
    "axios": "^0.16.2",
    "history": "^4.7.2",
    "lodash-es": "^4.17.4",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-loading": "^0.1.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
